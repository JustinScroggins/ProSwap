<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ProSwap</title>

    @Styles.Render("~/Content/css")
    @Styles.Render("~/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- CSS -->

    <!-- Font Awesome JS -->
    <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/solid.js"
            integrity="sha384-tzzSw1/Vo+0N5UhStP3bvwWPq+uvzCMfrN1fEFe+xBmv1C/AtVX5K0uZtmcHitFZ" crossorigin="anonymous">
    </script>
    <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/fontawesome.js"
            integrity="sha384-6OIrr52G08NpOFSZdxxz1xdNSndlD4vdcf/q2myIUVO0VsqaGHJsB0RaBE01VTOY" crossorigin="anonymous">
    </script>
    <script src="https://kit.fontawesome.com/8f994693f9.js" crossorigin="anonymous"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ProSwap Online Gaming Marketplace</title>

    <!-- Favicons -->

</head>

<body>

    <!-- header -->
    <div id="content">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container-fluid">
                @Html.ActionLink("ProSwap", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                <h1> test</h1>

        </nav>

        <nav class="navbar navbar-light bg-light">
            <a class="navbar-brand">ProSwap</a>
            <form class="form-inline">
                <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-outline-dark my-sm-0" type="submit">Search</button>

                <!--Social Icons-->
                <div class="social-icons">
                    <form class="form-inline my-2 my-lg-0">
                        <a href="https://twitter.com/itz4blitz/">
                            <i class="fab fa-twitter fa-lg"
                               style="color:#99aab5">
                            </i>
                            <a href="https://t.me/itz4blitz/">
                                <i class="fab fa-telegram-plane fa-lg"
                                   style="color:#99aab5">
                                </i>
                                <a href="https://discord.gg/PFtFC77">
                                    <i class="fab fa-discord fa-lg" style="color:#99aab5">
                                    </i>
                                    <a href="https://www.linkedin.com/in/justinscroggins/">
                                        <i class="fab fa-linkedin-in fa-lg" style="color:#99aab5">
                                        </i>
                                        <a href="https://github.com/JustinScroggins/">
                                            <i class="fab fa-github fa-lg"
                                               style="color:#99aab5">
                                            </i>
                                        </a>
                                    </a>
                                </a>
                            </a>
                        </a>
                    </form>
                </div>
    </div>
    </form>
    </nav>
    <!-- end header -->
    <div class="container body-content">
        @RenderBody()
        <hr />
        <!-- footer -->
        <!-- end footer -->

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)

    </div>
</body>

</html>
